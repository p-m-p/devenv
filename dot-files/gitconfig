[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = magenta
  untracked = cyan
[user]
	name = Phil Parsons
	email = p-m-p@users.noreply.github.com
[core]
	excludesfile = ~/.gitignore
	editor = /usr/bin/vim
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  pom = push origin master
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p
